{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-config-airbnb", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "ter-indent": 4,
        "no-empty-interface": false,
        "no-namespace": false,
        "quotemark": false,
        "max-line-length": [true, 120],
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "eofline": false,
        "ordered-imports": false,
        "align": false,
        "object-curly-spacing": false,
        "variable-name": false,
        "indent": [true],
        "import-name": false,
        "member-ordering": false,
        "function-name": [true, { "function-regex": "^[a-zA-Z][\\w\\d]+$", "static-method-regex": "^[A-Z][\\w\\d]+$" }]
    },
    "rulesDirectory": []
}
